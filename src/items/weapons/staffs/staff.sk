function staff_leftClick(p: player, staff: item):
  set {_id} to item_get_tag({_staff}, "ID")
  if {_id} is "LIMB_OF_WISDOM":
    limb_of_wisdom_cast({_p}, {_staff})

function staff_hit(p: player, id: text, location: location):
  if {_id} is "LIMB_OF_WISDOM":
    limb_of_wisdom_hit({_p}, {_location})